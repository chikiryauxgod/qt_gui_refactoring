+++    def open(self) -> bool:
+++        self._cap = cv2.VideoCapture(self._camera_idx)
+++        if not self._cap or not self._cap.isOpened():
+++            self._cap = None
+++            return False
+++
+++        self._cap.set(cv2.CAP_PROP_FRAME_WIDTH, self._width)
+++        self._cap.set(cv2.CAP_PROP_FRAME_HEIGHT, self._height)
+++        return True
