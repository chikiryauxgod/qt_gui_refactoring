+++            config.validate()
+++    
+++    def test_validate_zero_button_height(self):
+++        """Test validation fails with zero button height."""
+++        config = AxisControlConfig(button_height=0)
+++        
+++        with pytest.raises(ValueError, match="Button dimensions must be positive"):
+++            config.validate()
