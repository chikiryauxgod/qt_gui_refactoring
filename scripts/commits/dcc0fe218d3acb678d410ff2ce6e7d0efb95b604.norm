+++
+++    service.add_point((1.0, 2.0))
+++
+++    assert service.get_points() == [
+++        (0.0, 0.0),
+++        (1.0, 2.0),
+++    ]
+++
+++
+++def test_remove_joint_point():
+++    service = JointTrajectoryService()
+++    service.set_initial_point((0.0,))
+++    service.add_point((1.0,))
